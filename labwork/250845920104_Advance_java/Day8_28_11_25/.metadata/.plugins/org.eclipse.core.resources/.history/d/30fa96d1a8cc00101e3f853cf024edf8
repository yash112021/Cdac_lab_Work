package com.demo.beans;

public class User {
	private String usr;
	private String pass;
	
	// default constructor
	public User() {
		super();
	}
	
	
	// paramterised constructor 
	public User(String usr, String pass) {
		super();
		this.usr = usr;
		this.pass = pass;
	}

	// toString Method 
	@Override
	public String toString() {
		return "User [usr=" + usr + ", pass=" + pass + "]";
	}
}
